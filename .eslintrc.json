{
	"parser": "babel-eslint",
	"extends": "airbnb",
	"plugins": [
		"import"
	],
	"env": {
		"es6": true
	},
	"rules": {
		"no-underscore-dangle": [
			"error",
			{
				"allowAfterThis": true
			}
		],
		"no-plusplus": "off",
		"no-param-reassign": [
			"error",
			{
				"props": false
			}
		]
	}
}
